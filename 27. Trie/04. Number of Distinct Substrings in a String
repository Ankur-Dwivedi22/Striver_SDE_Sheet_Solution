/*You are required to complete this method */
class TrieNode{
    public:
        unordered_map<char, TrieNode*> child;
};

int countDistinctSubstring(string s) {
    // Your code here
    TrieNode* root = new TrieNode();
    
    int n = s.size() , ct = 0;
    for(int i=0; i<n; i++){
        TrieNode* temp = root;
        for(int j=i; j<n; j++){
            if(temp->child.find(s[j]) == temp->child.end()){
                ct++;
                temp->child[s[j]] = new TrieNode();
            }
            temp = temp->child[s[j]];
        }
    }
    
    return ct+1;
}